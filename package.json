    "zod": "^3.11.6"
  },
  "resolutions": {
    "@expo/config-plugins": "~7.2.2",
    "@expo/prebuild-config": "~6.2.4"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.0",
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.13",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "babel-preset-expo": "^9.5.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-universe": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "jest-date-mock": "^1.0.8",
    "jest-expo": "^44.0.0",
    "prettier": "^2.5.1",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^17.0.2",
    "sqlite3": "^5.0.0",
    "typescript": "^5.1.3",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "verbose": true,
    "restoreMocks": true,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.(spec|test).[jt]s?(x)"
    ],
    "setupFiles": [
      "<rootDir>/src/__tests__/setupGlobalVariables.js",
      "<rootDir>/src/__tests__/setupAsyncStorage.js",
      "<rootDir>/src/__tests__/setupSecureStore.ts",
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/src/__tests__/setup.ts"
    ],
    "globalSetup": "<rootDir>/src/__tests__/globalSetup.ts",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "App.tsx",
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/inc/**",
      "config/**/*.{js,jsx,ts,tsx}",
      "utility/**/*.{js,jsx,ts,tsx}",
      "!**/__tests__/**",
      "!**/node_modules/**"
    ]
  },
  "private": true,
  "packageManager": "yarn@3.1.1"
}
