{
  "name": "wellping",
  "version": "1.1.2",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test": "jest",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/slider": "4.1.12",
    "@wellping/study-schemas": "https://github.com/wellping/study-schemas",
    "date-fns": "^2.14.0",
    "expo": "^44.0.0",
    "expo-constants": "~13.0.0",
    "expo-crypto": "~10.1.1",
    "expo-device": "~4.1.0",
    "expo-file-system": "~13.1.0",
    "expo-linking": "~3.0.0",
    "expo-notifications": "~0.14.0",
    "expo-secure-store": "~11.1.0",
    "expo-splash-screen": "~0.14.0",
    "expo-sqlite": "~10.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-web-browser": "~10.1.0",
    "firebase": "9.6.1",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.19",
    "react": "17.0.1",
    "react-native": "0.64.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-sqlite-storage": "^5.0.0",
    "react-native-uuid": "^2.0.1",
    "react-native-webview": "11.15.0",
    "reflect-metadata": "^0.1.13",
    "seedrandom": "^3.0.5",
    "url": "^0.11.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.0",
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^7.0.1",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.13",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "babel-preset-expo": "9.0.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-universe": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-expo": "^44.0.0",
    "prettier": "^2.5.1",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^17.0.2",
    "sqlite3": "^5.0.0",
    "typescript": "~4.3.5",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "verbose": true,
    "restoreMocks": true,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.(spec|test).[jt]s?(x)"
    ],
    "setupFiles": [
      "<rootDir>/src/__tests__/setupGlobalVariables.js",
      "<rootDir>/src/__tests__/setupAsyncStorage.js",
      "<rootDir>/src/__tests__/setupSecureStore.ts",
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/src/__tests__/setup.ts"
    ],
    "globalSetup": "<rootDir>/src/__tests__/globalSetup.ts",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "App.tsx",
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/inc/**",
      "config/**/*.{js,jsx,ts,tsx}",
      "utility/**/*.{js,jsx,ts,tsx}",
      "!**/__tests__/**",
      "!**/node_modules/**"
    ]
  },
  "private": true,
  "packageManager": "yarn@3.1.1"
}
